@using Ignite.Models.ViewModels.Fitnesses
@model List<GetFitnessViewModel>

<link rel="stylesheet" href="~/css/FitnessesAll.css" />

<h1 id="AllFitnessesTitle">All Fitnesses</h1>

@foreach (var fitness in Model)
{
    <div class="fitnessWidget col-8">
        <h1>Ignite - @fitness.Name</h1>
        <h4>Address: @fitness.Address</h4>
    </div>
}

<h1 id="hqTitle">Company Headquaters</h1>

<iframe src="https://www.google.com/maps/d/embed?mid=1JYzzY65HQ9k8PpVsbchoWrN_WtHPW7LZ&ehbc=2E312F" width="800" height="540"></iframe>